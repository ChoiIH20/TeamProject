<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type 의 별칭을 등록 해 놓고  Mapper xml 에서 사용할수 있다. -->
	<typeAliases>
		<typeAlias type="com.one.project.users.dto.UsersDto" alias="usersDto"/>
		<typeAlias type="com.one.project.product.dto.ProductDto" alias="productDto"/>
	</typeAliases>
	<!-- 필요한 Mapper xml 등록하기  -->
	<mappers>
		<mapper resource="com/one/project/mybatis/UsersMapper.xml"/>
		<mapper resource="com/one/project/mybatis/BasketsMapper.xml"/>
		<mapper resource="com/one/project/mybatis/MasterMapper.xml"/>
		<mapper resource="com/one/project/mybatis/GenderMapper.xml"/>
		<mapper resource="com/one/project/mybatis/ProductMapper.xml"/>

	</mappers>
</configuration>